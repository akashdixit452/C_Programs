#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char ch;
    char str[50];
    char s[100];
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    scanf("%c",&ch); 
    printf("%c \n", ch);
    scanf("%s",str);
    printf("%s \n",str);
    scanf("\n");
    

    scanf("%[^\n]%*c", s);
    printf("%s \n",s);


    return 0;
}
